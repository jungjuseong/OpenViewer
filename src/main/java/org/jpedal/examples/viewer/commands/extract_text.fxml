<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10" scaleShape="false" styleClass="root" vgap="10" 
xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="org.jpedal.examples.viewer.commands.ExtractTextCoord2">

	<padding>
		<Insets bottom="25" left="25" right="25" top="25" />
	</padding>
   
   	<Label text="닷패턴 크기" GridPane.rowIndex="1">
   		<GridPane.margin>
   			<Insets left="10.0" />
   		</GridPane.margin>
   	</Label>
   	<ProgressBar fx:id="progressBar" prefHeight="18.0" prefWidth="130.0" progress="0.0" GridPane.columnIndex="1" />
   	<Label fx:id="progressLabel" prefHeight="15.0" prefWidth="47.0" text="진행:" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
   		<GridPane.margin>
   			<Insets left="20.0" />
   		</GridPane.margin>
   	</Label>
   	<ComboBox fx:id="dotPatternSize" prefHeight="23.0" prefWidth="87.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   
   	<HBox alignment="center" prefHeight="23.0" prefWidth="146.0" spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="2">
      <children>
      		<Button fx:id="extractButton" onAction="#handleTextExtract" text="추출하기" />
      		<Button fx:id="closeButton" onAction="#handleClose" text="닫기" />
      		
      </children>
   	</HBox>

</GridPane>
